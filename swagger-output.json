  "components": {
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "description": "JWT Authorization header using the Bearer scheme."
      }
    },
    "schemas": {
      "User": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com"
          },
          "password": {
            "type": "string",
            "example": "strongPassword123"
          },
          "firstName": {
            "type": "string",
            "example": "John"
          },
          "lastName": {
            "type": "string",
            "example": "Doe"
          },
          "profilePicture": {
            "type": "object",
            "properties": {
              "imageUrl": {
                "type": "string",
                "example": "https://example.com/avatar.jpg"
              },
              "publicId": {
                "type": "string",
                "example": "profile_public_id"
              }
            }
          },
          "phoneNumber": {
            "type": "string",
            "example": "+2348012345678"
          },
          "isVerified": {
            "type": "boolean",
            "example": false
          },
          "role": {
            "type": "string",
            "example": "student"
          },
          "sponsorType": {
            "type": "string",
            "example": "individual"
          },
          "organizationName": {
            "type": "string",
            "example": "Example Org"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "Academic": {
        "type": "object",
        "properties": {
          "studentId": {
            "type": "string",
            "example": "64d3f6a9e4b0f2a1d9c0b123"
          },
          "schoolName": {
            "type": "string",
            "example": "Example University"
          },
          "year": {
            "type": "number",
            "example": 2024
          },
          "matricNumber": {
            "type": "number",
            "example": 12345678
          },
          "jambRegistrationNumber": {
            "type": "number",
            "example": 87654321
          }
        },
        "required": [
          "studentId",
          "schoolName",
          "year",
          "matricNumber",
          "jambRegistrationNumber"
        ]
      },
      "Campaign": {
        "type": "object",
        "properties": {
          "studentId": {
            "type": "string",
            "example": "64d3f6a9e4b0f2a1d9c0b123"
          },
          "title": {
            "type": "string",
            "example": "Help fund my tuition"
          },
          "target": {
            "type": "number",
            "example": 500000
          },
          "story": {
            "type": "string",
            "example": "A short description of the student's need and campaign goals."
          },
          "isActive": {
            "type": "boolean",
            "example": true
          },
          "campaignImage": {
            "type": "object",
            "properties": {
              "imageUrl": {
                "type": "string",
                "example": "https://example.com/campaign.jpg"
              }
            }
          },
          "publicId": {
            "type": "string",
            "example": "campaign_public_id"
          }
        },
        "required": [
          "studentId",
          "title",
          "target",
          "story"
        ]
      },
      "Otp": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "example": "64d3f6a9e4b0f2a1d9c0b123"
          },
          "otp": {
            "type": "string",
            "example": "123456"
          },
          "trials": {
            "type": "number",
            "example": 0
          },
          "blockedUntil": {}
        },
        "required": [
          "userId",
          "otp"
        ]
      },
      "Payment": {
        "type": "object",
        "properties": {
          "campaignId": {
            "type": "string",
            "example": "64d3f6a9e4b0f2a1d9c0b999"
          },
